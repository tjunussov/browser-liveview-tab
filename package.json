{
  "name": "browser-liveview-tab",
  "displayName": "browser-liveview-tab",
  "description": "Open Brwoser LiveView Tab in VSCode",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjunussov/browser-liveview-tab.git"
  },
  "homepage": "https://github.com/tjunussov/browser-liveview-tab/blob/master/README.md",
  "publisher": "TIMTJEYJUN",
  "engines": {
    "vscode": "^1.25.0"
  },
  "author": {
    "email": "tjunussov@gmail.com",
    "name": "Timur Junussov"
  },
  "maintainers": [
    {
      "email": "tjunussov@gmail.com",
      "name": "Timur Junussov"
    }
  ],
  "galleryBanner": {
    "color": "#29b768",
    "theme": "light"
  },
  "keywords": [
    "chrome",
    "browser",
    "tab",
    "dizoft"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.browserliveviewtab"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.browserliveviewtab",
        "title": "Open Browser LiveView Tab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.34"
  }
}
